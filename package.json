{
  "name": "n8n-data-service",
  "version": "1.0.0",
  "description": "A service for handling data from n8n webhooks.",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "NODE_ENV=development ts-node src/app.ts",
    "start:prod": "NODE_ENV=production node dist/app.js",
    "build": "tsc",
    "test": "jest",
    "docker:build": "docker build -t n8n-data-service:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
