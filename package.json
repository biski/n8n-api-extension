    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsdom": "^27.2.0",
    "morgan": "^1.10.1",
    "youtube-caption-extractor": "^1.9.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/jsdom": "^27.0.0",
    "@types/morgan": "^1.9.10",
    "jest": "^27.0.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
